          //-----------------------------------------------------------------BlazeCreditBureauDataArray ends ----------------------------------------------------------------------------------------------
        }
        reportCounter = reportCounter +1;
      }      
    }
    aBlazeApplicantArray.append(aBlazeApplicant);
  }  
  
  // blazeApplicantArray
  print("----> aBlazeApplicantArray count :: " aBlazeApplicantArray.count);
