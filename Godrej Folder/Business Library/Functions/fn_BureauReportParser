            for each Enquiry in creditReport.Enquiry 
            do {
              enquiryArrayObj is some EnquiryArray initially a EnquiryArray;
              if(it.EnquiryPurpose <> unknown and it.EnquiryPurpose <> "") then {
                enquiryArrayObj.purpose = it.EnquiryPurpose;
